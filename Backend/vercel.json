{
  "version": 2,
  "builds": [
    { "src": "api/index.js", "use": "@vercel/node" },
    { "src": "api/scheduler.js", "use": "@vercel/node" }
  ],
  
  "routes": [
    { "src": "/(.*)", "dest": "api/index.js" }
  ],
  "crons": [
    {
      "path": "/api/scheduler",
      "schedule": "*/5 * * * *"  
    }
  ]
}
